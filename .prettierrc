{
    "plugins": ["prettier-plugin-astro"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ],
    "tabWidth": 4,
    "printWidth": 999999,
    "singleAttributePerLine": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "objectWrap": "preserve",
    "singleQuote": true,
    "semi": true,
    "useTabs": false,
    "endOfLine": "auto"
}
